                                                            <div class="description__heading-block">
                                                                <div class="description__heading-icon">
                                                                    <img src="~/Assets/classiceu/images/Pirate.png" alt="bottom-heading-block-icon">
                                                                </div>
                                                                <div class="description__heading-text">
                                                                    Scheduled Competitions
                                                                    <span>
                                                                        Siege and Olympiad
                                                                    </span>
                                                                </div>
                                                            </div>
                                                            <div class="description__content">
                                                                <div class="description__banner">
                                                                    <div class="news__pattern">
