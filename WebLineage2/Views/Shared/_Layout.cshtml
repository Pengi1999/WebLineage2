                                            <ul class="menu-area-main">
                                                <li class="@Html.Raw(ViewBag.active=="Home" ? "active":"")"> <a href="~/Home">Home</a> </li>
                                                <li class="@Html.Raw(ViewBag.active=="Features" ? "active":"")"> <a href="~/Home/Features">Features</a> </li>
